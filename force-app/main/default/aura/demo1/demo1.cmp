<aura:component >
    <aura:attribute name= "xyz" type= "string" default="test value"/>
    <aura:handler name ="init" value = "{!this}" action = "{!c.doInit}"/>
    <aura:attribute name = "newAcc" type = "Account" default = "{'sobjectType':'Account','Name':'TestAcc1'}"/>
    <h1> {!v.newAcc.Name}</h1>
    <hr/>
    <aura:attribute name = "arrayDemo" type = "String[]" default = "['v1','v8','v7']"/>
    Array Values:{!v.arrayDemo[0]}{!v.arrayDemo[1]}{!v.arrayDemo[2]}
    <hr/>
    <aura:attribute name = "listDemo" type = "List" default = "['list1','list8','list7']"/>
    List Values:{!v.listDemo[0]}{!v.listDemo[1]}{!v.listDemo[2]}
    <hr/>
    <aura:attribute name = "setDemo" type = "Set" default = "['setv1','setv8','setv7']"/>
    Set Values:{!v.setDemo[0]}{!v.setDemo[1]}{!v.setDemo[2]}
    <hr/>
    <aura:attribute name = "mapDemo" type = "Map" default = "{v1:'Map2',v8:'Map3',v7:'Map4'}"/>
    MapValues:{!v.mapDemo.v7}{!v.mapDemo.v8}
    <hr/>
    length of List:{!v.listDemo.length}
    length of List:{!v.setDemo.length}
    length of List:{!v.mapDemo.length}
    <hr/>
     <h1> Component Attribute'sValue:{!v.xyz}</h1>
    <hr/>
	<p style="font-size:50px;">Hello this is my first page</p>
<p style="font-size:30px;color:blue;"><i>My first paragraph</i></p>
    <h1 style="font-family:verdana;font-size:30px">This is a heading</h1>
<p style="font-family:courier;font-size:30px">This is a paragraph.</p>
<div style="max-width:95%;">
  <img style="max-width:85%;max-height:90%; height: 220px; width: 480px;" src="https://thumbs.dreamstime.com/b/blue-gerbera-flower-isolated-png-format-white-background-macro-photograph-137393818.jpg;"/> 
</div>
</aura:component>